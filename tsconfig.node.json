{
    "extends": "@vitejs/plugin-react/tsconfig",
    "compilerOptions": {
        "composite": true,
        "lib": [
            "ES2020",
            "DOM",
            "DOM.Iterable"
        ],
        "module": "ESNext",
        "target": "ES2020",
        "types": [
            "vite/client"
        ]
    },
    "include": [
        "**/*.ts",
        "**/*.tsx",
        "**/*.js",
        "**/*.jsx"
    ],
    "references": [
        {
            "path": "./tsconfig.json"
        }
    ]
}